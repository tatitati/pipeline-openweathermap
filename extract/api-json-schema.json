{
   "$schema":"http://json-schema.org/draft-04/schema#",
   "title":"User",
   "type":"object",
   "properties":{
      "id":{
         "type":"integer"
      },
      "name":{
         "type":"string"
      },
      "main": {
         "type": "object",
         "properties": {
            "temp":{
               "type": "number"
            },
            "feels_like": {
               "type": "number"
            },
            "temp_min": {
               "type": "number"
            },
            "temp_max": {
               "type": "number"
            },
            "pressure": {
               "type": "number"
            },
            "humidity": {
               "type": "number"
            }
         }
      }
   },
   "required":[
      "id",
      "name",
      "main"
   ]
}